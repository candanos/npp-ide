{
  "openapi": "3.0.1",
  "info": {
    "description": "A development operations service for zos.",
    "version": "1.0.0",
    "title": "benZoic",
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "externalDocs": {
    "description": "Find out more about benZoic",
    "url": "http://benzoic.io"
  },
  "servers": [
    {
      "url": "https://benzoic.io/v2"
    },
    {
      "url": "http://benzoic.io/v2"
    }
  ],
  "tags": [
    {
      "name": "build",
      "description": "Your build operations on zOS",
      "externalDocs": {
        "description": "Find out more",
        "url": "http://benzoic.io/build"
      }
    },
    {
      "name": "deployment",
      "description": "Your deployment operations on zOS"
    },
    {
      "name": "unitTest",
      "description": "Run your unit Tests on ZOS",
      "externalDocs": {
        "description": "Find out more",
        "url": "http://benzoic.io/unittest"
      }
    }
  ],
  "paths": {
    "/build/useAnArchiveFile": {
      "get": {
        "tags": [
          "zip"
        ],
        "summary": "build an archive file.",
        "description": "You should provide resources in an archive file.",
        "operationId": "buildUsingAnArchiveFile",
        "parameters": [
          {
            "name": "location",
            "in": "query",
            "description": "A full path in the USS.",
            "required": true,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/xml": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Pet"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Pet"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid status value",
            "content": {}
          }
        },
        "security": [
          {
            "petstore_auth": [
              "write:pets",
              "read:pets"
            ]
          }
        ]
      }
    },
  },
  "components": {
    "schemas": {
      "Result": {
        "required": [
          "name",
          "status"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "doggie"
          },
          "photoUrls": {
            "type": "array",
            "xml": {
              "name": "photoUrl",
              "wrapped": true
            },
            "items": {
              "type": "string"
            }
          },
          "tags": {
            "type": "array",
            "xml": {
              "name": "tag",
              "wrapped": true
            },
            "items": {
              "$ref": "#/components/schemas/Tag"
            }
          },
          "status": {
            "type": "string",
            "description": "pet status in the store",
            "enum": [
              "available",
              "pending",
              "sold"
            ]
          }
        },
        "xml": {
          "name": "Pet"
        }
      },
      "ApiResponse": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32"
          },
          "type": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "petstore_auth": {
        "type": "oauth2",
        "flows": {
          "implicit": {
            "authorizationUrl": "http://benzoic.io/oauth/dialog",
            "scopes": {
              "write:pets": "modify pets in your account",
              "read:pets": "read your pets"
            }
          }
        }
      },
      "api_key": {
        "type": "apiKey",
        "name": "api_key",
        "in": "header"
      }
    }
  }
}