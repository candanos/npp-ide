      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
	"version": "1.0.0",
	
  },
  "externalDocs": {
    "description": "Find out more about benZoic",
    "url": "http://benzoic.io"
  },
  "servers": [
    {
      "url": "https://benzoic.io/v2"
    },
    {
      "url": "http://benzoic.io/v2"
    }
  ],
  "tags": [
    {
      "name": "build",
      "description": "Your build operations on ZOS",
      "externalDocs": {
        "description": "Find out more",
        "url": "http://benzoic.io/build"
      }
    },
    {
      "name": "deployment",
      "description": "Your deployment operations on ZOS"
    },
    {
      "name": "unitTest",
      "description": "Run your unit Tests on ZOS",
      "externalDocs": {
        "description": "Find out more",
        "url": "http://benzoic.io/unittest"
      }
    }
  ],
  "paths": {
    "/build/useAnArchiveFile": {
      "get": {
        "tags": [
          "build"
        ],
        "summary": "build an archive file.",
        "description": "You should provide resources in an archive file.",
        "operationId": "buildUsingAnArchiveFile",
        "parameters": [
          {
            "name": "location",
            "in": "query",
            "description": "A full path in the USS.",
            "required": true,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/JobResult"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid status value",
            "content": {}
          }
        }
      }
    },
  },
  "components": {
    "schemas": {
	  "JobResult": {
			"type": "object",
			"properties":{	
				"step": {
					"type": "string",
				},
				"status": {
					"type": "string"
				}
			}
		},
      "ApiResponse": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32"
          },
          "type": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        }
      }
    
    },
	 "securitySchemes": {
      "petstore_auth": {
        "type": "oauth2",
        "flows": {
          "implicit": {
            "authorizationUrl": "http://petstore.swagger.io/oauth/dialog",
            "scopes": {
              "write:pets": "modify pets in your account",
              "read:pets": "read your pets"
            }
          }
        }
      },
      "api_key": {
        "type": "apiKey",
        "name": "api_key",
        "in": "header"
      }
    }
  }
}