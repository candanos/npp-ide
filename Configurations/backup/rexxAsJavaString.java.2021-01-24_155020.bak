lines.add("            Do kx=1 To isfddname.0                                  ")
lines.add("               ADDRESS MVS                                          ")
lines.add("              "EXECIO * DISKR" isfddname.kx "(STEM line. FINIS"     ")
lines.add("               Do dd=1 To line.0                                    ")
lines.add("                  Say line.dd                                       ")
lines.add("               End                                                  ")
lines.add("			   Res=0                                                 "  )
lines.add("            End                                                     ")
lines.add("        End                                                         ")
lines.add("      End                                                           ")
lines.add("   End                                                              ")
lines.add("End                                                                 ")
lines.add("If DATATYPE(Res)="NUM" Then Do                                      ")
lines.add("   Exit Res                                                         ")
lines.add("End                                                                 ")
lines.add("Else Do                                                             ")
lines.add("   Exit 93                                                          ")
lines.add("End                                                                 ")
   
