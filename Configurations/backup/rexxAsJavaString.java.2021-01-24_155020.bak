lines.add("/* Rexx */                                                          ")
lines.add("ADDRESS TSO                                                         ")
lines.add("Arg Jname Jid                                                       ")
lines.add("                                                                    ")
lines.add("FilterVal = Jname\'(\'Jid\')\'                                      ")
lines.add("/*         */                                                       ")
lines.add("cnt=1                                                               ")
lines.add("do cnt=1 To 100                                                     ")
lines.add("   Drop St.                                                         ")
lines.add("   X1=OutTrap(\'St.\')                                              ")
lines.add("   Status FilterVal                                                 ")
lines.add("   X1=OutTrap('OFF')                                                ")
lines.add("   JOBSTAT=Strip(St.1)                                              ")
lines.add("   if Rc=0 Then Do                                                  ")
lines.add("     Stat3=Word(St.1,3)                                             ")
lines.add("     Stat4=Word(St.1,4)                                             ")
lines.add("     Select                                                         ")
lines.add("     When Stat3='ON'        Then Leave                              ")
lines.add("     When Stat4='ON'        Then Leave                              ")
lines.add("     When Stat3='EXECUTING' Then NOP                                ")
lines.add("     When Stat4='EXECUTING' Then NOP                                ")
lines.add("     When Stat3='WAITING'   Then NOP                                ")
lines.add("     When Stat4='WAITING'   Then NOP                                ")
lines.add("     OtherWise Exit 91                                              ")
lines.add("     End                                                            ")
lines.add("   End                                                              ")
lines.add("   Else Do                                                          ")
lines.add("      Exit 91                                                       ")
lines.add("   End                                                              ")
lines.add("   cnt=cnt+1                                                        ")
lines.add("   BEKLE=MPFWAIT(0001)                                              ")
lines.add("End                                                                 ")
lines.add("/*          */                                                      ")
lines.add("BEKLE=MPFWAIT(0001)                                                 ")
lines.add("Drop JNAME.                                                         ")
lines.add("Drop line.                                                          ")
lines.add("Drop isfddname.                                                     ")
lines.add("JFilter='JOBID'                                                     ")
lines.add("PROFILE NOPREFIX                                                    ")
lines.add("RC = Isfcalls(ON)                                                   ")
lines.add("ADDRESS SDSF                                                        ")
lines.add("isfprefix = '**'                                                    ")
lines.add("isfowner = '**'                                                     ")
lines.add("isffilter = "'"JFilter"'"  " EQ '"Jid"'"                            ")
lines.add(""ISFEXEC H(delayed)"                                                ")
lines.add("                                                                    ")
lines.add("Res = 92                                                            ")
lines.add("Do ix=1 To JNAME.0                                                  ")
lines.add("   "ISFACT H TOKEN('"TOKEN.ix"') PARM(NP ?)"                        ")
lines.add("   Do rg = 1 To isfrows2                                            ")
lines.add("      If DDNAME.rg='JESMSGLG' Then Do                               ")
lines.add("        "ISFACT H TOKEN('"TOKEN.rg"') PARM(NP SA)"                  ")
