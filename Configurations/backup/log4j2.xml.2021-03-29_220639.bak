<Configuration status="debug">
    <Appenders>
        <File name="file-log" fileName="/isbank/sbteknik/ttg/devopz-zos-rest-server.log" append="false">
            <PatternLayout>
                <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %C - %msg%n</pattern>
            </PatternLayout>
        </File>

    </Appenders>
    <Loggers>
        <Logger name="com.anishek" level="info" additivity="false">
            <appender-ref ref="file-log" level="info"/>
        </Logger>
    </Loggers>
</Configuration>