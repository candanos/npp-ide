/* Rexx */

Drop St.
X1=OutTrap('St.')
Status Jobn
Rc1=Rc
X1=OutTrap('OFF')  
JOBSTAT=Strip(St.1)
do Forever
 if Rc1=0 Then Do 
   Stat3=Word(St.1,3)
   Stat4=Word(St.1,4)
   Select                                              
   When Stat3='ON'        Then Leave                
   When Stat4='ON'        Then Leave                
   When Stat3='EXECUTING' Then NOP     
   When Stat4='EXECUTING' Then NOP         
   When Stat3='WAITING'   Then NOP         
   When Stat4='WAITING'   Then NOP         
   OtherWise Return 1                      
   End                                                         
 End                                        
 Else Return 1                       
End

