fileNameWithExt=${ussPath##*/}
ext=${fileNameWithExt##*.}
name=${fileNameWithExt%%.*}

start="//'"
end="'"
targetPath=$start$mvsPath$end

log="$base/log/$name""Deploy.log"
cp -v -X -S d=.$ext $ussPath $targetPath 1>> $log 2>> $log
echo $?

read -p "Press [Enter] key to go on." 
exit $?

