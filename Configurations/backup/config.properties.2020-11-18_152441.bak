sqlGetRepositoriesOfCopies=SELECT DISTINCT MR.MBR_REPO FROM SCDEGISK.MEMBER_REPO MR WHERE MR.MBR_NAME IN (SELECT DISTINCT CP.COPYNAME FROM SCDEGISK.CPYFIELD CP WHERE CP.PROGNAME IN (<member_list>))
#
sqlGetRepoOfMember=SELECT DISTINCT MBR_REPO FROM SCDEGISK.MEMBER_REPO WHERE MBR_NAME = '<member_name>'
#
sqlGetCopiesOfMembers=SELECT DISTINCT CP.COPYNAME FROM SCDEGISK.CPYFIELD CP, SCDEGISK.MODULS m WHERE m.PGMNAME = CP.PROGNAME AND CP.PROGNAME IN (<member_list>)
#
sqlGetDclsOfMembers=SELECT DISTINCT D.DCLNAME FROM SCDEGISK.DCLFIELD D , SCDEGISK.MODULS m WHERE m.PGMNAME = D.PROGNAME AND D.PROGNAME IN (<member_list>)
#
